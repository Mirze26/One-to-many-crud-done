@model Event


@{
    ViewData["Title"] = "Detail";
}



<div style="padding-left:300px" class="container my-5">


   
    <h4>Title</h4>
    <p>@Model.Title</p>
    <h4>Description</h4>
    <p>@Model.Description</p>
